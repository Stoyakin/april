.social
  display inline-flex
  flex-wrap wrap
  align-items center
  &--dark
    .social__item-href
      &--inst
        background-image url('data:image/png;base64,')
      &--ytb
        background-image url('data:image/png;base64,')
  &__item
    margin-right 25px
    &:last-child
      margin-right 0
    &-href
      display block
      width 30px
      height 30px
      background-repeat no-repeat
      background-position center
      transition opacity .5s
      &:hover
        opacity .7
      &--inst
        background-image url('data:image/png;base64,')
      &--ytb
        background-image url('data:image/png;base64,')
